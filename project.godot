config_version=5

[application]

config/name="UntitledTamagotchiGame"
run/main_scene="res://Scenes/LaCreaturaScene.tscn"
config/features=PackedStringArray("4.4", "Mobile")

[autoload]

GameManager="*res://Singletons/Service/GameManager.gd"
ConfigurationManager="*res://Singletons/Service/ConfigurationManager.gd"
NavigationManager="*res://Singletons/Service/NavigationManager.gd"
DatabaseManager="*res://Singletons/Service/DatabaseManager.gd"
ActivityManager="*res://Singletons/Service/ActivityManager.gd"
PlayerManager="*res://Singletons/PlayerManager.gd"
StatsManager="*res://Singletons/StatsManager.gd"
FilePaths="*res://Singletons/FilePaths.gd"
SaveManager="*res://Singletons/Service/SaveManager.gd"
CoopSoccerManager="*res://Activities/CoopSoccer/scripts/coop_soccer_manager.gd"

[display]

window/size/mode=2
window/stretch/mode="canvas_items"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/godot-sqlite/plugin.cfg", "res://addons/gut/plugin.cfg", "res://addons/sprite_painter/plugin.cfg")

