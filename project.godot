; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="UntitledTamagotchiGame"
run/main_scene="res://Scenes/LaCreaturaScene.tscn"
config/features=PackedStringArray("4.4", "Mobile")

[autoload]

GameManager="*res://Singletons/Service/GameManager.gd"
UiManager="*res://Singletons/Service/UIManager.gd"
FileManager="*res://Singletons/Service/FileManager.gd"
DatabaseManager="*res://Singletons/Service/DatabaseManager.gd"
ActivityManager="*res://Singletons/Service/ActivityManager.gd"
Player="*res://Singletons/Player.gd"
StatsManager="*res://Singletons/StatsManager.gd"

[editor]

version_control/plugin_name="GitPlugin"
version_control/autoload_on_startup=true

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/godot-sqlite/plugin.cfg")

[rendering]

renderer/rendering_method="mobile"
